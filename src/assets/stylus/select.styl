.select-box {
  width 100%
  max-height 800px
  overflow-y auto
  .select-item {
    padding: 32px 0px 20px 40px;
    .title{
      font-size: 28px;
      color: #454b54;
      line-height: 40px;
      margin-bottom: 20px;
      font-weight:bold;
    }
    .content{
      margin-top 0
      ul{
        overflow hidden
        li {
          float: left;
          width: 150px;
          margin-right: 20px;
          margin-bottom: 30px;
          text-align: center;
          color: #999;
          border-radius: 10px;
          line-height: 60px;
          border: 2px solid #eee;
          font-size: 26px;

          &.selected {
            color: #56c4fe !important;
            border-color: #56c4fe !important;
          }
        }
      }
    }
  }
}
  .detail-title{
      border-bottom 1px solid #eee
      display flex
      .kuai{
        margin-left 10px
        margin-top 24px
        width 10px
        height 50px
        background #3b92fc
      }
      .text{
        margin-left 10px
        flex 8
        line-height 100px
        height 100px
        font-size 30px
        color #999
      }
      .year{
        flex 2
      }
    }
    .detail-content{
      padding 30px
    }
.application-box {
  overflow hidden
  background #f4f4f7
  .content {
    width: 100%;
    margin-top: -50px
    .b_item {
      position: relative;
      margin: 30px;
      padding: 30px;
      background: #fff;
      border-radius: 10px;
      img {
        width: 75px;
        height: 75px;
      }
      .complete {
        position: absolute;
        width: 75px;
        height: 75px;
        right: 20px;
        top: 50px;
      }
      .order {
        color: #2c85fd;
        font-weight: bold;
        font-size: 45px;
      }
      .order span {
        font-size: 60px;
      }
      .detail {
        margin-top: 30px;
        font-size: 35px;
        font-weight: 400;
        color: rgba(153, 153, 153, 1);
        line-height: 30px;
        span{
          margin-left: 5px;
          margin-right: 5px;
          color: #ff961a;
          font-size: 40px;
        }
      }
    }
    .btn {
      position: relative;
      text-align: center;
      line-height: 90px;
      font-size: 36px;
      margin: 0 30px;
      margin-bottom: 100px;
      height: 90px;
      color: #fff;
      background: rgba(53, 166, 253, 1);
      border-radius: 4px;
      img{
        width: 40px;
        height: 40px;
        position: absolute;
        left: 200px;
        top: 25px;
      }
    }
  }
}